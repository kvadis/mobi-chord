        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

  <!-- cdn scripts -->
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.5/angular.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.5/angular-animate.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.5/angular-sanitize.js"></script>
  <script src="https://unpkg.com/ng-table@2.0.2/bundles/ng-table.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js"></script>
  <script src="https://cdn.jsdelivr.net/angular.chartjs/latest/angular-chart.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/2.5.0/ui-bootstrap.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/2.5.0/ui-bootstrap-tpls.js"></script>
	
  <!-- css -->
  <link rel="stylesheet" href="https://unpkg.com/ng-table@2.0.2/bundles/ng-table.min.css"/>
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-9gVQ4dYFwwWSjIDZnLEWnxCjeSWFphJiwGPXr1jddIhOegiu1FwO5qRGvFXOdJZ4"
        crossorigin="anonymous"/>
	
  <script src="x_20458_mobi_chord.mainController.jsdbx"></script>

  <link href="bc95f274db211300d1dcfcdfbf9619e4.cssdbx" rel="stylesheet" type="text/css"></link>

  <g:evaluate object="true">
    var g_ck = gs.getSession().getSessionToken();
  </g:evaluate>
  <script>
    window.g_ck = "$[g_ck]";
  </script>

  <!-- The User Interface -->
  <div id="wrapper">
    <div id="header">
      <h1 class="bold">Incidents</h1>
      <hr/>
    </div>
    <div id="container" ng-app="angularapp">
      <div id="top">
        <div id="top-left" ng-controller="DoughnutCtrl as doughnut">
          <h4>Incidents By State</h4>
          <canvas id="doughnut" class="chart chart-doughnut" chart-data="data" chart-labels="labels" chart-options="options"></canvas>
        </div>
        <div id="top-right" ng-controller="LineCtrl as linectrl">
          <h4>Incidents Over Time</h4>
			<canvas id="line" class="chart chart-line" chart-data="data" chart-labels="labels" chart-series="series" chart-options="options" chart-dataset-override="datasetOverride" chart-click="onClick">
			</canvas>
        </div>
      </div>
      <div id="center" ng-controller="MainCtrl as main">
		  
        <table ng-table="main.tableParams" class="table table-striped">
          <tr ng-repeat="row in $data track by row.number">
            <td data-title="'Incident Number'" ng-class="'bold'" filter="{number: 'text'}" sortable="'number'">{{row.number}}</td>
            <td data-title="'Urgency'">{{row.urgency}}</td>
            <td data-title="'Caller'" filter="{caller_id: 'text'}" sortable="'caller_id'">{{row.caller_id}}</td>
            <td data-title="'Opened'">{{row.opened_at | date:'dd/MM/yyyy'}}</td>
            <td data-title="'State'" ng-class="'bold'">{{row.state}}</td>
          </tr>
        </table>
      </div>
		<div id="footer" ng-controller="appController">
		  <button class="btn btn-default" ng-click="openDialog()">Open Dialog</button>
		</div>
<!-- 		<p ng-controller="SampleController"><a href="" class="btn btn-default btn-lg " ng-click="showCustom()">Show Custom</a></p> -->
    </div>
    <div id="footer">
	</div>
  </div>


</j:jelly>
]]></html>
